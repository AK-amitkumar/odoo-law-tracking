"   % endif\n"
"   % if object.entry_date:\n"
"   &nbsp;&nbsp;Fecha de ingreso: <strong>${object.format_date(object.entry_date,\"%d/%m/%Y\")}</strong><br />\n"
"   % endif\n"
"   &nbsp;&nbsp;Categorías: <strong>\n"
"   % for category in object.law_category_ids:\n"
"    ${category.name}, \n"
"   % endfor\n"
"    </strong><br />\n"
"   % if object.legislature_type=='bicameral':\n"
"   &nbsp;&nbsp;Cámara de entrada: <strong>${object.law_project_type_id.name}</strong><br />\n"
"   % endif\n"
